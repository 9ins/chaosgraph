package org.chaos.graph;
/**
* <p>Title: NotMatchGraphTypeException</p>
* <p>Description:</p>
* <pre>
* 그래프 종류가 일치하지 않을 경우 발생하는 Exception
* </pre>
* <p>Copyright: Copyleft (c) 2006</p>
* <p>Company: ChaosToCosmos</p>
* @author 9ins
* @version 1.0, 2001/8/13 19:30 최초작성
* @version 1.2, 2006/7/5 오류문자열 변수를 추가함
*/
public class NotMatchGraphTypeException extends RuntimeException
{
    /**
     * 오류 문자열
     * @since JDK1.4.1
     */
    public static String ERR_STR = "차트의 종류가 일치하지 않습니다.";
    /**
     * 디폴트 생성자
     * @since JDK1.4.1
     */
    public NotMatchGraphTypeException()
    {
        super(ERR_STR);
    }
    /**
     * 오류 메시지를 인자로 갖는 생성자
     * @param str String 메시지
     * @since JDK1.4.1
     */
    public NotMatchGraphTypeException(String str)
    {
        super(str);
    }
}
