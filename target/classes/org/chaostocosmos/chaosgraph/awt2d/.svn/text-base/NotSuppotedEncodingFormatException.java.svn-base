package org.chaos.graph;
/**
* <p>Title: NotSuppotedEncodingFormatException</p>
* <p>Description:</p>
* <pre>
* 지원하지 않는 엔코드 포멧일시 발생하는 Exception
* </pre>
* <p>Copyright: Copyleft (c) 2006</p>
* <p>Company: ChaosToCosmos</p>
* @author 9ins
* @version 1.2, 2006/7/5 최초작성
*/
public class NotSuppotedEncodingFormatException extends Exception
{
    /**
     * 오류 문자열
     * @since JDK1.4.1
     */
    public static String ERR_STR = "지원하지 않는 엔코딩 포멧 입니다.";
    /**
     * 디폴트 생성자
     * @since JDK1.4.1
     */
    public NotSuppotedEncodingFormatException()
    {
        super(ERR_STR);
    }
    /**
     * 오류 메시지를 인자로 갖는 생성자
     * @param str String 메시지
     * @since JDK1.4.1
     */
    public NotSuppotedEncodingFormatException(String str)
    {
        super(str);
    }
}
